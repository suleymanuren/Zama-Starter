#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/suley/Documents/GitHub/Zama-Starter/frontend/node_modules/.pnpm/@zama-fhe+relayer-sdk@0.2.0/node_modules/@zama-fhe/relayer-sdk/bin/node_modules:/mnt/c/Users/suley/Documents/GitHub/Zama-Starter/frontend/node_modules/.pnpm/@zama-fhe+relayer-sdk@0.2.0/node_modules/@zama-fhe/relayer-sdk/node_modules:/mnt/c/Users/suley/Documents/GitHub/Zama-Starter/frontend/node_modules/.pnpm/@zama-fhe+relayer-sdk@0.2.0/node_modules/@zama-fhe/node_modules:/mnt/c/Users/suley/Documents/GitHub/Zama-Starter/frontend/node_modules/.pnpm/@zama-fhe+relayer-sdk@0.2.0/node_modules:/mnt/c/Users/suley/Documents/GitHub/Zama-Starter/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/suley/Documents/GitHub/Zama-Starter/frontend/node_modules/.pnpm/@zama-fhe+relayer-sdk@0.2.0/node_modules/@zama-fhe/relayer-sdk/bin/node_modules:/mnt/c/Users/suley/Documents/GitHub/Zama-Starter/frontend/node_modules/.pnpm/@zama-fhe+relayer-sdk@0.2.0/node_modules/@zama-fhe/relayer-sdk/node_modules:/mnt/c/Users/suley/Documents/GitHub/Zama-Starter/frontend/node_modules/.pnpm/@zama-fhe+relayer-sdk@0.2.0/node_modules/@zama-fhe/node_modules:/mnt/c/Users/suley/Documents/GitHub/Zama-Starter/frontend/node_modules/.pnpm/@zama-fhe+relayer-sdk@0.2.0/node_modules:/mnt/c/Users/suley/Documents/GitHub/Zama-Starter/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/relayer.js" "$@"
else
  exec node  "$basedir/../../bin/relayer.js" "$@"
fi
